    "fs-extra": "5.0.0",
    "get-folder-size": "1.0.1",
    "glob": "7.1.2",
    "gm": "1.23.1",
    "google-closure-compiler-js": "20170218.0.0",
    "gzip-size": "4.1.0",
    "http-server": "0.11.1",
    "husky": "0.14.3",
    "klaw-sync": "3.0.2",
    "lint-staged": "3.2.5",
    "lodash": "4.17.5",
    "markdown-doctest": "0.9.1",
    "minimist": "1.2.0",
