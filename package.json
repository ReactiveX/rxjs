    "@types/node": "9.4.5",
    "@types/sinon": "4.1.3",
    "@types/sinon-chai": "2.7.29",
    "babel-polyfill": "6.26.0",
    "benchmark": "2.1.0",
    "benchpress": "2.0.0-beta.1",
    "chai": "4.1.2",
    "color": "3.0.0",
    "colors": "1.1.2",
    "commitizen": "2.9.6",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "cz-conventional-changelog": "1.2.0",
