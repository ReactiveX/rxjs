    "systemjs": "^0.21.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "tslint-no-unused-expression-chai": "0.0.3",
    "typescript": "latest",
    "validate-commit-msg": "2.14.0",
    "watch": "1.0.2",
    "webpack": "1.13.1",
    "xmlhttprequest": "1.8.0"
  },
  "engines": {
    "npm": ">=2.0.0"
  },
  "typings": "./dist/package/Rx.d.ts"
}
