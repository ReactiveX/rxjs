  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.0.24",
    "@types/chai": "^4.0.4",
    "@types/lodash": "^4.14.80",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.47",
    "@types/sinon": "^2.3.7",
    "@types/sinon-chai": "^2.7.29",
    "babel-polyfill": "^6.23.0",
    "benchmark": "^2.1.0",
    "benchpress": "2.0.0-beta.1",
    "chai": "^3.5.0",
    "color": "^0.11.1",
    "colors": "1.1.2",
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.13",
