    "jasmine-marbles": "^0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "jasmine-ts": "^0.4.0",
    "jsdom": "^15.1.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lighthouse": "^7.0.1",
    "lodash": "^4.17.20",
    "lunr": "^2.1.0",
    "protractor": "~7.0.0",
    "rehype-slug": "^2.0.3",
    "remark": "^12.0.1",
    "remark-html": "^13.0.2",
    "rimraf": "^2.6.1",
    "semver": "^6.1.1",
    "shelljs": "^0.8.3",
    "stemmer": "^2.0.0",
    "svgo": "^1.3.2",
    "svgson": "^4.1.0",
    "tree-kill": "^1.2.2",
    "ts-node": "^8.2.0",
    "typescript": "~5.7.3",
    "unist-util-filter": "^1.0.2",
    "unist-util-source": "^1.0.5",
    "unist-util-visit": "^1.4.1",
    "unist-util-visit-parents": "^2.1.2",
    "watchr": "^4.1.0",
