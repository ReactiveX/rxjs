    "@types/node": "^20.11.0",
    "@types/svgo": "^1.3.3",
    "@types/trusted-types": "^2.0.7",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
