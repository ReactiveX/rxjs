    "archiver": "^3.0.0",
    "canonical-path": "^1.0.0",
    "chalk": "^2.1.0",
    "cjson": "^0.5.0",
    "concurrently": "^5.3.0",
    "cross-spawn": "^6.0.5",
    "css-selector-parser": "^1.3.0",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.30.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.23.4",
